#include <stdio.h>

int swap(int *a,int *b){//initialised the addresses of a and b
    int temp=*a;
    *a = *b;
    *b = *a;//swapped the addresses of a and b
    *b=temp;
}
int main() {
    int j = 2, k = 5;
    printf("j=%d, k=%d\n",j,k);
    swap (&j,&k);// called the function of swap
    printf("j=%d, k=%d\n",j,k,swap);
    return 1;
}
